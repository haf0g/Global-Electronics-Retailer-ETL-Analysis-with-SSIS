-- Create Customers table
CREATE TABLE dbo.Customers (
    CustomerKey INT PRIMARY KEY,
    Gender NVARCHAR(10),
    Name NVARCHAR(100),
    City NVARCHAR(100),
    StateCode NVARCHAR(10),
    State NVARCHAR(100),
    ZipCode NVARCHAR(10),
    Country NVARCHAR(100),
    Continent NVARCHAR(100),
    Birthday DATE
);

CREATE TABLE dbo.Stores (
    StoreKey INT PRIMARY KEY,
    Country NVARCHAR(100),
    State NVARCHAR(100),
    SquareMeters INT,
    OpenDate DATE
);

